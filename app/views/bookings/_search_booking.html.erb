<div class="row" id="search-booking">
  <div class="col s4 offset-s4">
    <form action="<%= search_booking_path %>" method="get">
      <div class="search search-wrapper card" id="search-parent">
        <input placeholder="Booking code" name="booking_code" id="search" type="text" class="validate">
      </div>
      <button class="btn-floating btn-large waves-effect waves-light blue" type="submit" name="action">
        <i class="material-icons">search</i>
      </button>
    </form>
  </div>
</div>
